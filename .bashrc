# easier
alias t="tmux"
alias n="nano"
alias s="source ~/.bashrc"
alias rc="nano ~/.bashrc"
alias py='python3'
alias copy='xclip -selection clipboard'
alias ovpn="sudo openvpn"

alias push="git push"
alias pull="git pull"
alias gitd="git add .; git commit -m '[+]'; git push; echo '[*] Files Saved'" # git done
alias getg='echo "YOUR_GITHUB_TOEKEN" | xclip -selection clipboard'
alias gitback="git reset --soft HEAD~1"
